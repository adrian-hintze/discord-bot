{
    "name": "discord-bot",
    "version": "1.0.0",
    "description": "Small discord bot",
    "private": true,
    "author": {
        "name": "Adrian Hintze",
        "email": "hintze.adrian@gmail.com",
        "url": "https://github.com/adrian-hintze"
    },
    "engines": {
        "node": "12.10.0",
        "npm": "6.10.3"
    },
    "scripts": {
        "start": "node ./build/bot/Main.js",
        "deploy": "npm install && npm run build && npm run forever",
        "prebuild": "rimraf ./build/bot/",
        "build": "tsc",
        "forever": "forever start ./build/bot/Main.js",
        "stop": "forever stopall"
    },
    "dependencies": {
        "@discordjs/uws": "11.149.1",
        "discord.js": "11.5.1",
        "erlpack": "0.1.2",
        "express": "4.17.3",
        "image-downloader": "3.5.0",
        "is-image": "3.0.0",
        "is-image-url-async": "1.4.0",
        "mkdirp": "0.5.1",
        "node-opus": "0.2.7",
        "rimraf": "3.0.0",
        "valid-url": "1.0.9",
        "zlib-sync": "0.1.4"
    },
    "devDependencies": {
        "@types/express": "4.17.1",
        "@types/mkdirp": "0.5.2",
        "@types/node": "12.7.5",
        "@types/valid-url": "1.0.2",
        "typescript": "3.6.3"
    }
}
